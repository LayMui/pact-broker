{
  "name": "kcp-api",
  "version": "1.0.0",
  "description": "Consumer Pact",
  "main": "index.js",
  "scripts": {
    "test:pact": "jest cross-env CI=true __pact-tests__/ --runInBand --setupFiles ./pactSetup.js  --setupFilesAfterEnv ./pactTestWrapper.js --testMatch \"**/*.test.pact.js\"",
    "publish:pact": "CI=true node ./publishPacts.js",
    "verify:pact": "PACT_PUBLISH_RESULTS=true npx jest --detectOpenHandles --testMatch \"**/verify.pact.js\""
  },
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^9.2.1",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jest": "^24.9.0"
  },
  "dependencies": {
    "tree": "^0.1.3"
  }
}
