{
  "name": "kcp-api",
  "version": "1.0.0",
  "description": "Consumer Pact",
  "main": "index.js",
  "scripts": {
    "test:pact": "jest cross-env CI=true __pact-tests__/ --runInBand --setupFiles ./pactSetup.js  --setupFilesAfterEnv ./pactTestWrapper.js --testMatch \"**/*.test.pact.js\"",
    "publish:pact": "CI=true node ./publishPacts.js --broker-base-url $PACT_BROKER_URL",
    "verify:pact": "PACT_PUBLISH_RESULTS=true npx jest --testTimeout=15000 --detectOpenHandles --testMatch \"**/verify.pact.js\"",
    "create:provider:tag": "npx pact-broker create-version-tag --pacticipant iProvider --version 1.0.0 --tag staging --broker-base-url $PACT_BROKER_URL",
    "create:consumer:tag": "npx pact-broker create-version-tag --pacticipant iConsumer --version 1.0.0 --tag staging --broker-base-url $PACT_BROKER_URL",
    "can:i:deploy:consumer": "npx pact-broker can-i-deploy --pacticipant iConsumer --version 1.0.0 --pacticipant iProvider --latest staging --broker-base-url $PACT_BROKER_URL"
  },
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^9.11.1",
    "@pact-foundation/pact-node": "^10.9.7",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jest": "^24.9.0"
  },
  "dependencies": {
    "tree": "^0.1.3"
  }
}
